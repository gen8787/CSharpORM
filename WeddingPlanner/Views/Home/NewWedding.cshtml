@model Wedding

<div class="container">
    <h3>Plan Your Wedding</h3>
    <a asp-action="Dashboard">Dashboard</a>
    <div class="form border border-primary p-1 shadow-lg p-3 mb-5 rounded">
    <form asp-action="CreateWedding" asp-controller="Home" method="POST">
        <div class="form-group">
            <label for="WedderOne">Wedder One: </label>
            <input class=”form-control” asp-for="WedderOne">
            <small id="errors" class="form-text text-danger"><span asp-validation-for="WedderOne"></span></small>
        </div>
        <div class="form-group">
            <label for="WedderTwo">Wedder Two: </label>
            <input class=”form-control” asp-for="WedderTwo">
            <small id="errors" class="form-text text-danger"><span asp-validation-for="WedderTwo"></span></small>
        </div>
        <div class="form-group">
            <label for="Date">Date: </label>
            <input class=”form-control” asp-for="Date">
            <small id="errors" class="form-text text-danger"><span asp-validation-for="Date"></span></small>
        </div>
        <div class="form-group">
            <label for="Address">Address: </label>
            <input class=”form-control” asp-for="Address">
            <small id="errors" class="form-text text-danger"><span asp-validation-for="Address"></span></small>
        </div>
        <button class="btn btn-primary text-center" type="submit">Create</button>
    </form>
</div>